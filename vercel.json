{
  "version":2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
{
  "functions": {
    "api/index.js": {
      "maxDuration": 30 // This function can run for a maximum of 30 seconds
    },
    "api/index.js": {
      "maxDuration": 15 // This function can run for a maximum of 15 seconds
    }
  }
}